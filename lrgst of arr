#include <stdio.h>
int main()
{ 
    int n,arr[10];
    printf("enter the element : ");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        printf("enter the number  %d :",i);
        scanf("%d",&arr[i]);
    }
    for(int i=1;i<n;i++)
    {
     if(arr[0]<arr[i])
        {
      
      (arr[0]=arr[i]);
        }
    }
    printf("largest element : %d",arr[0]);
    
    return 0;
}
