#include <stdio.h>

int main()
{
    int n,sum=0;
    printf("enter the num : ");
    scanf("%d",&n);
    for (int i=1;i<=n/2;i++)
    {
        if(n%i==0)
        {
            sum = sum + i;
            
        }
    }
        if (sum == n)
          {
            printf("True!");
           }
        else
        {
            printf("False!");
        }
    return 0;
}

