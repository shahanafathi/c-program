#include <stdio.h>

int main()
{
    int num,remainder,temp,total;
    printf("Enter the value : ");
    scanf("%d",&num);
    temp=num;
    while (num>0)
    {
        remainder=num%10;
        total=total+(remainder*remainder*remainder);
        num=num/10;
    }
    if (temp==total)
    {
        printf("its a armstrome number ");
        
    }
    else
       printf("its not a armstrome number");
    return 0;
}
