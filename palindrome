#include <stdio.h>

int main()
{
    int n,rev,sum=0,temp;
    printf("Enter your number : ");
    scanf("%d",&n);
    temp=n;
    while(n>0)
    {
        rev=n%10;
        sum=(sum*10)+rev;
        n=n/10;
    }
    if(temp==sum)
    {
        printf("Its a palindrome!");
    }
    else
        printf("its not a palindrome");
        
    return 0;
}
